# Temperature Monitor Configuration
# Copy this file to .env and fill in your actual values

# AWS API Gateway Configuration
TEMPERATURE_MONITOR_API_KEY=your_api_key_here
TEMPERATURE_MONITOR_API_URL=https://your-api-gateway-url.execute-api.region.amazonaws.com/prod

# Security Configuration
ALLOWED_ORIGINS=localhost,127.0.0.1,yourdomain.com

# Request Configuration
REQUEST_TIMEOUT=10000
MAX_RETRIES=3

# Environment
NODE_ENV=production