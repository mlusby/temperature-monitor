# Temperature Monitor Configuration
# Copy this file to .env and fill in your actual values

# AWS API Gateway Configuration
TEMPERATURE_MONITOR_API_URL=https://your-api-gateway-url.execute-api.region.amazonaws.com/prod

# AWS Cognito Configuration
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_USER_POOL_CLIENT_ID=your_client_id_here
AWS_REGION=us-east-1

# Security Configuration
ALLOWED_ORIGINS=localhost,127.0.0.1,yourdomain.com

# Request Configuration
REQUEST_TIMEOUT=10000
MAX_RETRIES=3

# Environment
NODE_ENV=production